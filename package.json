{
  "name": "@kaanmertkoc/iyzico-subs-ts",
  "version": "1.0.6",
  "description": "Modern TypeScript SDK for Iyzico Subscription API - Cross-platform compatible",
  "author": "Kaan Mert Ko√ß <kaanmertkocbursa@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaanmertkoc/iyzico-subscription-ts"
  },
  "bugs": {
    "url": "https://github.com/kaanmertkoc/iyzico-subscription-ts/issues"
  },
  "homepage": "https://github.com/kaanmertkoc/iyzico-subscription-ts#readme",
  "keywords": [
    "iyzico",
    "subscription",
    "payment",
    "typescript",
    "sdk",
    "api",
    "cross-platform",
    "node",
    "bun",
    "deno",
    "serverless"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "private": false,
  "workspaces": [
    "examples/*"
  ],
  "exports": {
    "types": "./dist/types/index.d.ts",
    "browser": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "bun": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "deno": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "worker": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "workerd": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "default": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "bun run build:clean && bun run build:esm && bun run build:cjs && bun run build:types",
    "build:clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "typecheck": "tsc --noEmit",
    "dev": "bun --cwd examples/hono-app run dev",
    "dev:install": "cd examples/hono-app && bun install",
    "docs:sync": "cp openapi/openapi.yaml openapi/docs/openapi.yaml",
    "docs:serve": "bun --hot openapi/docs/server.ts",
    "docs:open": "open openapi/docs/index.html",
    "size": "bun scripts/check-package-size.ts",
    "size:track": "bun scripts/check-package-size.ts track",
    "size:history": "bun scripts/check-package-size.ts history",
    "prepublishOnly": "bun run build",
    "publish:npm": "npm publish --access public",
    "release:github": "gh release create v$(node -p \"require('./package.json').version\") --generate-notes",
    "release": "bun run publish:npm && bun run release:github"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
