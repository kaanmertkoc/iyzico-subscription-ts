{
  "name": "@kaanmertkoc/iyzico-subs-ts",
  "version": "1.0.0",
  "description": "Modern TypeScript SDK for Iyzico Subscription API - Cross-platform compatible",
  "author": "Kaan Mert Ko√ß <kaanmertkoc@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaanmertkoc/iyzico-subscription-ts"
  },
  "bugs": {
    "url": "https://github.com/kaanmertkoc/iyzico-subscription-ts/issues"
  },
  "homepage": "https://github.com/kaanmertkoc/iyzico-subscription-ts#readme",
  "keywords": [
    "iyzico",
    "subscription",
    "payment",
    "typescript",
    "sdk",
    "api",
    "cross-platform",
    "node",
    "bun",
    "deno",
    "serverless"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "private": false,
  "exports": {
    "types": "./dist/types/index.d.ts",
    "browser": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "bun": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "deno": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "worker": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "workerd": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "default": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "typecheck": "tsc --noEmit",
    "dev": "npm --prefix examples/hono-app run dev",
    "dev:install": "cd examples/hono-app && npm install",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}